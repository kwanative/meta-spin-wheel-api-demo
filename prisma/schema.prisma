generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model m_level_point {
  id          Int      @id @default(autoincrement())
  level_no    Int
  level_name  String
  point       Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
